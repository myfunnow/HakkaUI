{
  "name": "hakka-ui",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "main": "dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/*.d.ts",
        "dist/*/index.d.ts"
      ]
    }
  },
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./components": {
      "import": "./dist/components/index.mjs",
      "types": "./dist/components/index.d.ts"
    },
    "./package.json": "./package.json",
    "./main.css": "./dist/assets/styles/main.css"
  },
  "files": [
    "dist",
    "readme.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run build:src && npm run build:types",
    "build:src": "npx unbuild",
    "build:types": "vue-tsc --project tsconfig.build-types.json",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "postinstall": "npm run build"
  },
  "peerDependencies": {
    "naive-ui": "^2.37.3",
    "vue": "^3.3.0"
  },
  "dependencies": {
    "naive-ui": "^2.37.3",
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/types": "^7.23.9",
    "@storybook/addon-actions": "^7.6.16",
    "@storybook/addon-essentials": "^7.6.16",
    "@storybook/addon-interactions": "^7.6.16",
    "@storybook/addon-links": "^7.6.16",
    "@storybook/testing-library": "0.2.2",
    "@storybook/vue3-vite": "^7.6.16",
    "@types/node": "^20.11.19",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vitest/ui": "^1.3.0",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.1",
    "babel-loader": "^9.1.3",
    "jsdom": "^24.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "sass": "^1.71.0",
    "storybook": "^7.6.16",
    "storybook-dark-mode": "^3.0.3",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "unocss": "^0.58.5",
    "vite": "^4.5.2",
    "vite-svg-loader": "^4.0.0",
    "vitest": "^1.3.0",
    "vue": "^3.4.19",
    "vue-loader": "^17.4.2",
    "vue-tsc": "^1.8.27"
  }
}
